			<exclude name="Payment_Modes_Validation_withCustomer"/>
			<exclude name="Payment_Modes_Validation_withoutCustomer"/>
			<exclude name="Validation_of_InvoiceGeneration_with_AdvanceReceivedPaymentMode"/>
			<exclude name="Validation_of_PurchaseInvoiceGeneration_with_AdvancedPaidMode"/>
			</methods>
			</class>
			
			<class name="JBWindows_Configurations.SalesConfiguration_Test">
			<methods>
			<exclude name="OrderCreation_with_SalesOrdertype_as_HomeDelivery"/>
			<exclude name="EstimationConversion_with_SalesOrdertype_as_HomeDelivery"/>
			</methods>
			</class> 
			
			
