			<exclude name="Validation_of_InvoiceGeneration_withCustomer_through_CashPaymentmode"/>
			<exclude name="Validation_of_InvoiceGeneration_withoutCustomer_through_CashPaymentmode"/>
			<exclude name="Validation_of_PurchaseInvoiceGeneration"/>
