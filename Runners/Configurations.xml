					<exclude name="Show_ItemEditpopup_while_Billing_ReturnInvoice_generation_without_enabling_the_option"/>
					
				</methods>
			</class>
			
			<class name="JBWindows_Configurations.AppUsersTest">
			<methods>
			<exclude name="fnVerifyFieldVisibility_FieldEnabledorNot"/>
			<exclude name="fnUserCreation"/>
			<exclude name="fnUserEdit"/>
			</methods>
